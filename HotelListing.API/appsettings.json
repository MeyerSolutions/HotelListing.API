{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Mocrosoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/HotelListing.API-.txt",
          "rollingInterval": "Day"
        }
      }
      //,
      //{
      //  "Name": "Seq",
      //  "Args": { "serverUrl":  "http://localhost:5341" }
      //}
    ]
  },
  "AllowedHosts": "*"
}
